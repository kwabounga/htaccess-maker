{
  "name": "htaccess-maker",
  "version": "1.3.1",
  "scripts": {
    "postinstall": "electron-builder install-app-deps && patch-package",
    "ng": "ng",
    "start": "concurrently \"ng serve\" \"npm run electron\"",
    "electron": "electron ./src/electron.dev",
    "rebuild": "electron-rebuild -f -w sqlite3",
    "build": "ng build",
    "build:and:install": "npm run build && cross-var npm i --prefix ./dist/$npm_package_name/ ",
    "watch": "ng build --watch --configuration development",
    "package:win": "npm run build:and:install && cross-var electron-packager dist/$npm_package_name $npm_package_name-$npm_package_version --out=packages --platform=win32 --arch=all --overwrite ",
    "package:linux": "npm run build:and:install && cross-var electron-packager dist/$npm_package_name $npm_package_name-$npm_package_version --out=packages --platform=linux --arch=all --overwrite ",
    "package:osx": "npm run build:and:install && cross-var electron-packager dist/$npm_package_name $npm_package_name-$npm_package_version --out=packages --platform=darwin --arch=all --overwrite ",
    "package:all": "npm run build:and:install && cross-var electron-packager dist/$npm_package_name $npm_package_name-$npm_package_version --out=packages --all --arch=all --overwrite ",
    "create:scope:config": "node ./src/commands/cmd-create-scope-config.js",
    "create:database": "sqlite3 ./src/data/database.db \".databases .quit\" && node ./src/electron/build_db.js",
    "create:backup": "node ./src/commands/cmd-create-backup.js",
    "check:urls": "node ./src/commands/cmd-check-old-redirect.js",
    "logger:truncate": "node ./src/commands/cmd-logger-truncate.js",
    "c:u": "npm run check:urls",
    "c:b": "npm run create:backup",
    "c:db": "npm run create:database",
    "c:s:c": "npm run create:scope:config",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "@popperjs/core": "^2.11.5",
    "bootstrap": "^5.1.3",
    "createjs-module": "^0.8.3",
    "crypto-js": "^4.1.1",
    "electron-builder": "^23.1.0",
    "electron-rebuild": "^3.2.8",
    "electron-store": "^8.1.0",
    "knex": "^2.1.0",
    "ngx-electron": "^2.2.0",
    "ngx-smooth-dnd": "^0.4.1",
    "patch-package": "^6.4.7",
    "path-browserify": "^1.0.1",
    "raw-loader": "^4.0.2",
    "rxjs": "~7.5.0",
    "sqlite3": "^5.0.9",
    "tslib": "^2.3.0",
    "xml2js": "^0.4.23",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.6",
    "@angular/cli": "^14.1.0",
    "@angular/compiler-cli": "^14.0.0",
    "@types/bootstrap": "^5.2.4",
    "@types/jasmine": "~4.0.0",
    "concurrently": "^7.2.2",
    "cross-var": "^1.1.0",
    "electron": "^19.0.8",
    "electron-packager": "^15.5.1",
    "jasmine-core": "~4.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "react-app-rewired": "^2.2.1",
    "typescript": "~4.7.2"
  }
}
